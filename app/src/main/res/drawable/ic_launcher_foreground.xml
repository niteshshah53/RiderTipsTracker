<vector xmlns:android="http://schemas.android.com/apk/res/android"
    android:width="108dp"
    android:height="108dp"
    android:viewportWidth="108"
    android:viewportHeight="108">
    
    <!-- Delivery Bike - Simplified Design -->
    <!-- Front Wheel (outer circle) -->
    <path
        android:fillColor="#FFFFFF"
        android:pathData="M35,65 m-10,0 a10,10 0 1,1 20,0 a10,10 0 1,1 -20,0" />
    
    <!-- Front Wheel (inner circle) -->
    <path
        android:fillColor="#2E7D32"
        android:pathData="M35,65 m-6,0 a6,6 0 1,1 12,0 a6,6 0 1,1 -12,0" />
    
    <!-- Back Wheel (outer circle) -->
    <path
        android:fillColor="#FFFFFF"
        android:pathData="M70,65 m-10,0 a10,10 0 1,1 20,0 a10,10 0 1,1 -20,0" />
    
    <!-- Back Wheel (inner circle) -->
    <path
        android:fillColor="#2E7D32"
        android:pathData="M70,65 m-6,0 a6,6 0 1,1 12,0 a6,6 0 1,1 -12,0" />
    
    <!-- Bike Frame -->
    <path
        android:fillColor="#FFFFFF"
        android:strokeWidth="4"
        android:strokeColor="#FFFFFF"
        android:pathData="M35,65 L50,50 L70,65" />
    
    <!-- Handlebar -->
    <path
        android:fillColor="#FFFFFF"
        android:strokeWidth="4"
        android:strokeColor="#FFFFFF"
        android:pathData="M50,50 L45,45 M50,50 L45,55" />
    
    <!-- Seat -->
    <path
        android:fillColor="#FFFFFF"
        android:strokeWidth="3"
        android:strokeColor="#FFFFFF"
        android:pathData="M70,65 L75,60" />
    
    <!-- Money/Tips Symbol - Euro Sign Circle (outer) -->
    <path
        android:fillColor="#FFD700"
        android:pathData="M75,30 m-12,0 a12,12 0 1,1 24,0 a12,12 0 1,1 -24,0" />
    
    <!-- Euro Sign Horizontal Lines -->
    <path
        android:fillColor="#FFFFFF"
        android:strokeWidth="2.5"
        android:strokeColor="#FFFFFF"
        android:pathData="M68,30 L82,30 M68,35 L82,35" />
    
    <!-- Euro Sign Curved Part -->
    <path
        android:fillColor="#FFFFFF"
        android:strokeWidth="2.5"
        android:strokeColor="#FFFFFF"
        android:pathData="M70,25 Q75,25 75,30 Q75,35 70,35" />
    
</vector>
